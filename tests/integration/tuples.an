getCoords unit = (2, 3)

//Pack two params into a tuple
toTuple a b = (a, b)


printTuple name:Str t:(i32,i32) =
    printf "%s = (%d, %d)\n" (name.cStr) (t#0) (t#1)


coords = getCoords ()
printTuple "coords" coords

printTuple "toTuple(3, 2)" (toTuple 3 2)


