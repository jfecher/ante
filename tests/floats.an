`
        floats.an
    Tests compilation of various sized floats as well as
    floating point operations.
`

~IEEE double
f64 f1 = 0.125
f64 f2 = 1.0 / 8.0 * 6.0

printf("%.3f + %.3f = %.3f\n", f1, f2, f1 + f2)

~IEEE single
f32 s1 = 0.6f32
f32 s2 = 0.2f32 * 2.0f32
printf("%.2f * %.2f = %.2f\n", f64 s1, f64 s2, f64 s1 * f64 s2)

~IEEE half
f16 h1 = 0.5f16
f16 h2 = 1.5f16

printf("%.1f + %.1f = %.1f\n", f64 h1, f64 h2, f64 h1 + f64 h2)

