
effect Three with
    one: Unit -> Unit
    two: I32 -> I32
    three: I32 - I32 -> I32

effect State a with
    get: Unit -> a
    put: a -> Unit


handle ()
| one () -> ()
| one () -> ()


handle ()
| get () -> resume ()
| put () -> ()

handle ()
| put () -> ()
| three 0 1 -> ()

