type OptionI32 =
    | None
    | Some I32

OptionI32.unwrap_or_default (self: OptionI32) =
    match self
    | OptionI32.None -> 0
    | Some x -> x

shared type Tree t =
    | Leaf t
    | Node (Tree t) (Tree t)

Tree.map (self: Tree q) (f: fn q -> z): Tree z =
    match self
    | Leaf x -> Leaf (f x)
    | Node l r -> Node (l.map f) (r.map f)

